  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yinliguo/printer/issues"
  },
  "homepage": "https://github.com/yinliguo/printer#readme",
  "devDependencies": {
    "@types/node": "^16.4.5",
    "prettier": "2.3.2",
    "typescript": "^3.7.2",
    "vite": "^2.4.4"
  }
}
